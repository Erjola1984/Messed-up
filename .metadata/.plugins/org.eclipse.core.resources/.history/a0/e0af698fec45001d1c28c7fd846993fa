package com.neotech.lesson09;

public class ForLoopExtra {

	public static void main(String[] args) {
		
		System.out.println("Print numbers from 21 to 30");
			
		for(int i = 21; i <= 30;i++ );
	{
		System.out.print(i + " ");
		
	}
	 System.out.println();
	
	 
	 
	 
	 
	 
	 
	 
	 
	 
	 System.out.println("Print odd numbers from 1 to 10");
	 for(int i =1; i <=10; i+=2)
	 {
		System.out.println(i + " ");
	 }
	 
	 System.out.println("Print odd numbers from 1 to 10");//second way
	 
	 for (int i = 1; i <= 10; i++);
	 
	 //if i%2 ==1; i is odd number
	 //if i%2 ==o; i is even number
	 
	 if (i % 2 ==1)
	 {
		 
	 }System.out.println("\n Print the sum of numbers from 3 to 7"); 
		int sum= 0;
		for (int i = 3; i<7; i++);
		{
			sum +=i; // sum = sum + 1; 
		}
		System.out.println("Sum is: " + sum);
		
	
		 
	 }
	 
	 	 
	}
	
	

}
